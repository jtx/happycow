{
    "name": "jtx/happycow",
    "type": "project",
    "description": "A overly-engineered but... pretty decent version of this",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Services\\": "Services/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
